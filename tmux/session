new-window -n "root"
send-keys "ocenv" Enter
split-window -h -p 50
send-keys "ocenv" Enter

new-window -n "oc"
send-keys "cd oc" Enter
split-window -h -p 50
send-keys "cd oc" Enter

new-window -n "agent"
send-keys "cd oc/agent" Enter
split-window -h -p 50
send-keys "cd oc/agent" Enter

new-window -n "agent2"
send-keys "cd oc/agent2" Enter
split-window -h -p 50
send-keys "cd oc/agent2" Enter

new-window -n "prompts"
send-keys "cd oc/prompts" Enter
split-window -h -p 50
send-keys "cd oc/prompts" Enter

new-window -n "promptdata"
send-keys "cd oc/promptdata" Enter
split-window -h -p 50
send-keys "cd oc/promptdata" Enter

new-window -c "../onecommon/webapp/src" -n "server"
send-keys "ocenv" Enter
split-window -h -c "../onecommon/webapp/src" -p 50
send-keys "ocenv" Enter

new-window -c "../onecommon/aaai2020/experiments" -n "selfplay"
send-keys "ocenv" Enter
split-window -h -c "../onecommon/aaai2020/experiments" -p 50
send-keys "ocenv" Enter
